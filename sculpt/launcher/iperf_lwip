<launcher pkg="iperf">
	<route>
		<service name="Nic">
			<child name="nic_router"/>
		</service>
		<service name="Rtc">
			<child name="system_clock-pc"/>
		</service>
		<service name="RM">
			<parent/>
		</service>
	</route>

	<config>
		<arg value="iperf"/>
		<arg value="-s"/>
		<libc stdout="/dev/log" stderr="/dev/log" rtc="/dev/rtc" socket="/socket"/>
		<vfs>
			<dir name="dev">
				<log/> <rtc/>
			</dir>
			<dir name="socket">
				<lwip dhcp="yes"/>
			</dir>
		</vfs>
	</config>

</launcher>
